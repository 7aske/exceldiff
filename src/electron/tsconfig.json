{
	"compilerOptions": {
		"target": "es5",
		"module": "commonjs",
		"sourceMap": false,
		"outDir": "../../build",
		"removeComments": true,
		"strict": true,
		"strictFunctionTypes": true,
		"moduleResolution": "node",
		"typeRoots": [
			"/node_modules/@types/**.*"
		],
		"esModuleInterop": true
	}
}
